unt":0,"fixableWarningCount":0,"source":"88","usedDeprecatedRules":"44"},{"filePath":"89","messages":"90","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"91","usedDeprecatedRules":"44"},"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/index.js",[],["92","93"],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/App.js",["94"],"import React from 'react'\nimport Header from './components/Header'\nimport Main from './Main'\nimport \"./estilos.css\"\nimport { library } from '@fortawesome/fontawesome-svg-core'\nimport { faShoppingCart, faCoffee, faDoorClosed } from '@fortawesome/free-solid-svg-icons'\nimport ItemListContainer from './components/ItemListContainer'\nimport ItemDetailContainer from './components/ItemDetailContainer'\nimport { BrowserRouter, Switch, Route } from 'react-router-dom';\nimport Carrito from './components/Carrito'\nimport AboutMe from './components/AboutMe'\nimport CartState from './components/CartState'\n// import {firestore} from \"./firebaseConfig\"\n\n\nlibrary.add( faShoppingCart, faCoffee )\n\nconst App = () => {\n\n\n    return (\n        <>\n        <BrowserRouter>\n        <CartState>\n        <Header/>\n        <Switch>\n            <Route exact path=\"/\">\n                <Main/>\n                <ItemListContainer\n                greeting=\"Products\"\n                /> \n            </Route>\n            <Route exact path=\"/category/:categoryId\">\n                <ItemListContainer\n                />\n            </Route>\n            <Route exact path=\"/item/:id\">\n                <ItemDetailContainer/>\n            </Route>\n            <Route exact path=\"/ItemListContainer\">\n                <ItemListContainer\n                    greeting=\"Products\"\n                />\n            </Route>\n            <Route exact path=\"/cart\">\n                <Carrito/>\n            </Route>\n            <Route exact path=\"/aboutMe\">\n                <AboutMe/>\n            </Route>\n        </Switch>\n        </CartState>\n        </BrowserRouter>\n        </>\n    )\n}\n\nexport default App\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/Main.js",["95"],"import React from 'react'\nimport { Link } from 'react-router-dom'\n\n\nfunction Main() {\n \n    return (\n        <main>\n            <div className=\"title\">\n                <h1 className=\"titleText\">MEAL PLANS</h1>\n                <h3 className=\"titleText\">2 weeks of meals planned for you!</h3>\n                <Link to=\"/itemListContainer\"><button className=\"titleText mealButton\"><a>click to check meal plans</a></button></Link>\n                <div className=\"titleImg\">\n                    <img src=\"https://i.ibb.co/mTSRtJg/fondo1.jpg\" className=\"banner\" alt=\"banner\"></img>\n                    {/* <img src=\"../public/fondoResponsive.jpg\" className=\"bannerRes\" alt=\"banner\"></img> */}\n                </div>\n            </div>\n        </main>\n    )\n}\n\nexport default Main\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/ItemListContainer.js",["96"],"import React, { useState, useEffect } from 'react'\nimport ItemList from './ItemList'\nimport {useParams} from 'react-router-dom'\nimport { firestore } from \"../firebaseConfig\";\n\n\nconst ItemListContainer = ({greeting}) => {\n\n    const [items, setItems] = useState([]);\n    const { categoryId} = useParams();\n  \n    useEffect(() => {\n\n        let query;\n        categoryId?\n            query = firestore.collection(\"items\").where(\"categoryId\",\"==\", categoryId).get() :\n            query = firestore.collection(\"items\").get()\n\n        query.then(({docs}) =>{\n            setItems(docs.map( doc => ({id: doc.id, ...doc.data()})))\n        })\n    }, []);\n\n    return (\n        <div className=\"itemList\">\n        {categoryId ? <h1>{categoryId}</h1>\n        :<h1>{greeting}</h1>  \n        } \n          {items.length > 0 \n          ?<ItemList product = { items } /> \n          :<h1 className=\"loading\">Loading...</h1> }\n      </div>\n    )\n}\n\nexport default ItemListContainer\n\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/Carrito.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/ItemDetailContainer.js",["97"],"import React from 'react'\nimport GetItem from './GetItem'\n\n\nconst ItemDetailContainer = ({ }) => {\n    \n    return (\n        <div>\n            <GetItem/>\n        </div>\n    )\n}\n\nexport default ItemDetailContainer\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/Header.js",["98"],"import React from 'react'\nimport Navbar from './Navbar'\nimport { Link } from 'react-router-dom';\n\nconst Header = () => {\n    return (\n        <header>\n            <div className=\"introHeader\">\n               <Link to=\"/aboutMe\">\n                <a className=\"khyl\">Khyl Porcelli</a>\n               </Link> \n            </div>\n            <div className=\"header\"> \n                <Navbar/>\n            </div>\n        </header>\n    )\n}\n\nexport default Header\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/CartState.js",["99"],"// CartContext.js con el context y su custom provider (Impórtalo en App.js)\n// Al clickear comprar en ItemDetail se debe guardar en el CartContext el producto y su cantidad en forma de objeto { item: {} , quantity }\n// Detalle importante: CartContext debe tener la lógica incorporada de no aceptar duplicados y mantener su consistencia.\n// Métodos recomendados: \n// addItem(item, quantity) // agregar cierta cantidad de un ítem al carrito\n// removeItem(itemId) // Remover un item del cart por usando su id\n// clear() // Remover todos los items\n// isInCart: (id) => true|false\nimport React, {useState, useEffect} from 'react'\nimport CartContext from './CartContext'\n\nconst CartState = ({children}) => {\n\n    const [cart, setCart] = useState([])\n\n\n    function addToCart(item, cantidad) {\n\t\tsetCart([\n\t\t\t...cart,\n\t\t\t{\n                item: item,\n\t\t\t\tcantidad: cantidad\n\t\t\t}\n        ])\n    }\n    const isInCart = id => {\n        let existe = cart.find(producto => producto.item.id === id)\n        return existe?true:false\n    }\n    const deleteItem = id => {\n        const nuevoCart = cart.filter(producto => producto.item.id !== id)\n        setCart(nuevoCart)\n    }\n    const clearCart = () =>{\n        setCart([])\n    }\n\n    const updateCantidad = (id, cantidad) =>{\n        const oldCart = cart\n        const newCart = oldCart.map(p => {\n            if(p.item.id === id){\n                p.cantidad = cantidad\n            }\n            return p\n        })\n        setCart( newCart )\n    }\n    \n    const totalPrice = () => {\n        let contador = 0\n        cart.forEach(item => { contador = contador + (item.cantidad * item.price) })\n        return contador\n    }\n\n    const total = () => {\n        let contador = 0\n        cart.forEach(item => { contador = contador + item.cantidad })\n        return contador\n    }\n      \n    return (\n        <CartContext.Provider value={{\n            addToCart, \n            isInCart, \n            deleteItem, \n            clearCart, \n            updateCantidad,  \n            cart,\n            total,\n            totalPrice,\n            quantity: cart.length\n            }}>\n            {children}\n        </CartContext.Provider>\n    )\n}\n\nexport default CartState\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/AboutMe.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/CartContext.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/firebaseConfig.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/Navbar.js",["100","101"],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/ItemList.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/GetItem.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/CartItem.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/CartWidget.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/Item.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/DropdownNav.js",["102","103","104","105","106"],"import React from 'react'\nimport {Dropdown} from 'react-bootstrap';\nimport { Link } from 'react-router-dom'\n// import { getFirestore } from '../firebaseConfig';\n\n\nconst DropdownNav = () => {\n    \n    \n    return (\n        <>\n            <Dropdown>\n                <a className=\"listado\"><Link to=\"/ItemListContainer\">Meal Preps</Link>\n                <Dropdown.Toggle className=\"flechaCateg\"/>\n                </a>\n\n                <Dropdown.Menu className=\"MenuListado\">\n                   \n                    <Dropdown.Item className=\"categories\" >\n                        <Link to={`/category/Breakfast`}><a>Breakfast</a></Link> \n                    </Dropdown.Item>\n                    <Dropdown.Item className=\"categories\">\n                        <Link to={`/category/Dessert`}><a>Dessert</a></Link>\n                    </Dropdown.Item>\n                    <Dropdown.Item className=\"categories\">\n                        <Link to={`/category/PlantBased`}><a>Plant Based</a></Link>\n                    </Dropdown.Item>\n                    <Dropdown.Item className=\"categories\"> \n                        <Link to={`/category/ProteinBased`}><a>Protein Based</a></Link> \n                    </Dropdown.Item>\n        \n                </Dropdown.Menu>\n            </Dropdown>\n        </>\n    )\n}\n\nexport default DropdownNav\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/ItemDetail.js",["107"],"import React, { useContext } from 'react'\nimport ItemCount from './ItemCount'\nimport './itemDetail.css'\nimport CartContext  from './CartContext'\nimport { Link } from 'react-router-dom';\n\nconst ItemDetail = ({item}) => {\n\n    const { addToCart } = useContext(CartContext)\n   \n    return (\n        <>\n        <h1 className=\"titleDetail\"> {item.title} </h1> \n        <div className=\"itemDetail\">\n            <section className=\"itemBox\">\n                <img src ={ item.imagen } alt=\"alt\" className=\"itemImg\"></img>  \n                <p className=\"description\">{item.description}</p>\n            </section>\n            <section className=\"itemBox\">\n            <h1> ${item.price} </h1>\n            &nbsp;\n            { item.stock ? <h4>{item.stock} in stock</h4> :<h1>Not in stock</h1> }\n            &nbsp;\n            \n            <ItemCount \n                item={item}\n                addToCart = {addToCart}\n            /> \n            </section>  \n        </div>\n        </>\n    )\n}\n\nexport default ItemDetail\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/ItemCount.js",["108"],"import React, { useState, useContext } from 'react'\nimport { Link } from 'react-router-dom';\nimport CartContext from './CartContext'\n\n\nconst ItemCount = ({item, addToCart}) => {\n\n    const [ contDetail , setContDetail ] = useState(1)\n    const name = item.title\n    const { isInCart } = useContext(CartContext);\n    const existeItem = isInCart(item.id);\n\n    const aumentarCont = () => {\n        if (contDetail < item.stock){\n            setContDetail(contDetail + 1)\n        }else{\n            setContDetail( item.stock )\n        }        \n    }\n    const restarCont = () => {\n        if(contDetail > 1){\n            setContDetail(contDetail - 1)\n        }\n    }\n\n    return (\n        <>\n            { existeItem ?\n            <>\n            <h4>{\"agregaste\"+ \" \" + contDetail + \" \" + name + \" al carrito\" }</h4>\n            <Link to=\"/cart\">\n                <button className=\"addToCart\">ir al carrito</button>\n            </Link>\n            </>\n            :\n            <>\n            <div className=\"contador\">\n            <button onClick={restarCont} className=\"buttonProd\">-</button>\n            <p>{contDetail}</p>\n            <button onClick={aumentarCont} className=\"buttonProd\">+</button>\n            </div>    \n            <div>\n                <button className=\"addToCart\" \n                onClick={ () => {addToCart({ item: item, cantidad: contDetail})}}>\n                    add to cart</button>\n            </div>\n            </>}\n        </>\n    )\n}\n\nexport default ItemCount\n",{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","severity":1,"message":"114","line":6,"column":36,"nodeType":"115","messageId":"116","endLine":6,"endColumn":48},{"ruleId":"117","severity":1,"message":"118","line":12,"column":88,"nodeType":"119","endLine":12,"endColumn":91},{"ruleId":"120","severity":1,"message":"121","line":22,"column":8,"nodeType":"122","endLine":22,"endColumn":10,"suggestions":"123"},{"ruleId":"124","severity":1,"message":"125","line":5,"column":30,"nodeType":"126","messageId":"127","endLine":5,"endColumn":33},{"ruleId":"117","severity":1,"message":"118","line":10,"column":17,"nodeType":"119","endLine":10,"endColumn":37},{"ruleId":"113","severity":1,"message":"128","line":9,"column":26,"nodeType":"115","messageId":"116","endLine":9,"endColumn":35},{"ruleId":"117","severity":1,"message":"118","line":14,"column":31,"nodeType":"119","endLine":14,"endColumn":54},{"ruleId":"117","severity":1,"message":"118","line":15,"column":37,"nodeType":"119","endLine":15,"endColumn":60},{"ruleId":"117","severity":1,"message":"118","line":13,"column":17,"nodeType":"119","endLine":13,"endColumn":40},{"ru