op/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/ItemDetailContainer.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/AboutMe.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/firebaseConfig.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/ItemList.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/GetItem.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/Item.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/ItemDetail.js",["100"],"import React, { useContext, useState } from 'react'\nimport ItemCount from './ItemCount'\nimport './itemDetail.css'\nimport CartContext  from '../context/CartContext'\n// import { Link } from 'react-router-dom';\n\nconst ItemDetail = ({item}) => {\n\n    const { title, price, imagen, description} = item;\n\n    const { addToCart } = useContext(CartContext)\n   \n    return (\n        <>\n        <h1 className=\"titleDetail\"> {title} </h1> \n        <div className=\"itemDetail\">\n            <section className=\"itemBox\">\n                <img src ={ imagen } alt=\"alt\" className=\"itemImg\"></img>  \n                <p className=\"description\">{description}</p>\n            </section>\n            <section className=\"itemBox\">\n            <h1> ${price} </h1>\n            &nbsp;\n            { item.stock ? <h4>{item.stock} in stock</h4> :<h1>Not in stock</h1> }\n            &nbsp;\n            \n            <ItemCount \n                item={item}\n                addToCart = {addToCart}\n            /> \n            </section>  \n        </div>\n        </>\n    )\n}\n\nexport default ItemDetail\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/ItemCount.js",["101"],"import React, { useState, useContext } from 'react'\nimport { Link } from 'react-router-dom';\nimport CartContext from '../context/CartContext'\n\n\nconst ItemCount = ({item, addToCart}) => {\n\n    const [ contDetail , setContDetail ] = useState(1)\n    const name = item.title\n    const { isInCart } = useContext(CartContext);\n    const existeItem = isInCart(item.id);\n\n    const aumentarCont = () => {\n        if (contDetail < item.stock){\n            setContDetail(contDetail + 1)\n        }else{\n            setContDetail( item.stock )\n        }        \n    }\n    const restarCont = () => {\n        if(contDetail > 1){\n            setContDetail(contDetail - 1)\n        }\n    }\n\n    const onAdd = () => {\n        const add = addToCart({ item: item, cantidad: contDetail})\n        return  add\n    }\n    \n    return (\n        <>\n            { existeItem ?\n            <>\n            <h4>{\"agregaste\"+ \" \" + contDetail + \" \" + name + \" al carrito\" }</h4>\n            <Link to=\"/cart\">\n                <button className=\"addToCart\">ir al carrito</button>\n            </Link>\n            </>\n            :\n            <>\n            <div className=\"contador\">\n            <button onClick={restarCont} className=\"buttonProd\">-</button>\n            <p>{contDetail}</p>\n            <button onClick={aumentarCont} className=\"buttonProd\">+</button>\n            </div>    \n            <div>\n                <button className=\"addToCart\" \n                onClick={onAdd}>\n                    add to cart</button>\n            </div>\n            </>}\n        </>\n    )\n}\n\nexport default ItemCount\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/context/CartContext.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/header/Header.js",["102"],"import React from 'react'\nimport Navbar from './Navbar'\nimport { Link } from 'react-router-dom';\n\nconst Header = () => {\n    return (\n        <header>\n            <div className=\"introHeader\">\n               <Link to=\"/aboutMe\">\n                <a className=\"khyl\">Khyl Porcelli</a>\n               </Link> \n            </div>\n            <div className=\"header\"> \n                <Navbar/>\n            </div>\n        </header>\n    )\n}\n\nexport default Header\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/context/CartState.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/cart/Carrito.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/header/Navbar.js",["103"],"import React from 'react'\nimport CartWidget from './CartWidget'\nimport { Link, NavLink } from 'react-router-dom'\nimport './navbar.css'\nimport DropdownNav from './DropdownNav';\n\n\nconst Navbar = ()  => {\n\n    return (\n        <>\n        <nav>\n            <div className=\"lista\">\n                <Link to=\"/\" className=\"listado\">Home</Link>\n                <Link to=\"/aboutMe\" className=\"listado\">About Me</Link>\n                <DropdownNav/>\n                <CartWidget/>\n            </div>\n        </nav>\n        </>\n    )\n}\n\nexport default Navbar\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/cart/CartItem.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/header/DropdownNav.js",["104","105","106","107","108"],"import React from 'react'\nimport {Dropdown} from 'react-bootstrap';\nimport { Link } from 'react-router-dom'\n// import { getFirestore } from '../firebaseConfig';\n\n\nconst DropdownNav = () => {\n    \n    \n    return (\n        <>\n            <Dropdown>\n                <a className=\"listado\"><Link to=\"/ItemListContainer\">Meal Preps</Link>\n                <Dropdown.Toggle className=\"flechaCateg\"/>\n                </a>\n\n                <Dropdown.Menu className=\"MenuListado\">\n                   \n                    <Dropdown.Item className=\"categories\" >\n                        <Link to={`/category/Breakfast`}><a>Breakfast</a></Link> \n                    </Dropdown.Item>\n                    <Dropdown.Item className=\"categories\">\n                        <Link to={`/category/Dessert`}><a>Dessert</a></Link>\n                    </Dropdown.Item>\n                    <Dropdown.Item className=\"categories\">\n                        <Link to={`/category/PlantBased`}><a>Plant Based</a></Link>\n                    </Dropdown.Item>\n                    <Dropdown.Item className=\"categories\"> \n                        <Link to={`/category/ProteinBased`}><a>Protein Based</a></Link> \n                    </Dropdown.Item>\n        \n                </Dropdown.Menu>\n            </Dropdown>\n        </>\n    )\n}\n\nexport default DropdownNav\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/header/CartWidget.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/buyingForm.js",["109"],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/BuyingForm.js",[],{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","severity":1,"message":"115","line":12,"column":88,"nodeType":"116","endLine":12,"endColumn":91},{"ruleId":"117","severity":1,"message":"118","line":1,"column":29,"nodeType":"119","messageId":"120","endLine":1,"endColumn":37},{"ruleId":"121","severity":1,"message":"122","line":35,"column":29,"nodeType":"123","messageId":"124","endLine":35,"endColumn":30},{"ruleId":"114","severity":1,"message":"115","line":10,"column":17,"nodeType":"116","endLine":10,"endColumn":37},{"ruleId":"117","severity":1,"message":"125","line":3,"column":16,"nodeType":"119","messageId":"120","endLine":3,"endColumn":23},{"ruleId":"114","severity":1,"message":"115","line":13,"column":17,"nodeType":"116","endLine":13,"endColumn":40},{"ruleId":"114","severity":1,"message":"115","line":20,"column":58,"nodeType":"116","endLine":20,"endColumn":61},{"ruleId":"114","severity":1,"message":"115","