components/ItemListContainer.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/ItemDetailContainer.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/AboutMe.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/firebaseConfig.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/ItemList.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/GetItem.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/Item.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/ItemDetail.js",["101"],"import React, { useContext, useState } from 'react'\nimport ItemCount from './ItemCount'\nimport './itemDetail.css'\nimport CartContext  from '../context/CartContext'\n// import { Link } from 'react-router-dom';\n\nconst ItemDetail = ({item}) => {\n\n    const { title, price, imagen, description} = item;\n\n    const { addToCart } = useContext(CartContext)\n   \n    return (\n        <>\n        <h1 className=\"titleDetail\"> {title} </h1> \n        <div className=\"itemDetail\">\n            <section className=\"itemBox\">\n                <img src ={ imagen } alt=\"alt\" className=\"itemImg\"></img>  \n                <p className=\"description\">{description}</p>\n            </section>\n            <section className=\"itemBox\">\n            <h1> ${price} </h1>\n            &nbsp;\n            { item.stock ? <h4>{item.stock} in stock</h4> :<h1>Not in stock</h1> }\n            &nbsp;\n            \n            <ItemCount \n                item={item}\n                addToCart = {addToCart}\n            /> \n            </section>  \n        </div>\n        </>\n    )\n}\n\nexport default ItemDetail\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/ItemCount.js",["102"],"import React, { useState, useContext } from 'react'\nimport { Link } from 'react-router-dom';\nimport CartContext from '../context/CartContext'\n\n\nconst ItemCount = ({item, addToCart}) => {\n\n    const [ contDetail , setContDetail ] = useState(1)\n    const name = item.title\n    const { isInCart } = useContext(CartContext);\n    const existeItem = isInCart(item.id);\n\n    const aumentarCont = () => {\n        if (contDetail < item.stock){\n            setContDetail(contDetail + 1)\n        }else{\n            setContDetail( item.stock )\n        }        \n    }\n    const restarCont = () => {\n        if(contDetail > 1){\n            setContDetail(contDetail - 1)\n        }\n    }\n\n    const onAdd = () => {\n        const add = addToCart({ item: item, cantidad: contDetail})\n        return  add\n    }\n    \n    return (\n        <>\n            { existeItem ?\n            <>\n            <h4>{\"agregaste\"+ \" \" + contDetail + \" \" + name + \" al carrito\" }</h4>\n            <Link to=\"/cart\">\n                <button className=\"addToCart\">ir al carrito</button>\n            </Link>\n            </>\n            :\n            <>\n            <div className=\"contador\">\n            <button onClick={restarCont} className=\"buttonProd\">-</button>\n            <p>{contDetail}</p>\n            <button onClick={aumentarCont} className=\"buttonProd\">+</button>\n            </div>    \n            <div>\n                <button className=\"addToCart\" \n                onClick={onAdd}>\n                    add to cart</button>\n            </div>\n            </>}\n        </>\n    )\n}\n\nexport default ItemCount\n",["103","104"],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/context/CartContext.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/header/Header.js",["105"],"import React from 'react'\nimport Navbar from './Navbar'\nimport { Link } from 'react-router-dom';\n\nconst Header = () => {\n    return (\n        <header>\n            <div className=\"introHeader\">\n               <Link to=\"/aboutMe\">\n                <a className=\"khyl\">Khyl Porcelli</a>\n               </Link> \n            </div>\n            <div className=\"header\"> \n                <Navbar/>\n            </div>\n        </header>\n    )\n}\n\nexport default Header\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/context/CartState.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/cart/Carrito.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/header/Navbar.js",["106","107","108"],"import React from 'react'\nimport CartWidget from './CartWidget'\nimport {Dropdown} from 'react-bootstrap';\nimport { Link, NavLink } from 'react-router-dom'\nimport './navbar.css'\nimport DropdownNav from './DropdownNav';\n\n\nconst Navbar = ()  => {\n\n    const categorias = [{key: 'plantBased', description:'Plant Based'}, {key: 'proteinBased', description:'Protein Based'}, {key: 'dessert', description:'Dessert'}, {key: 'breakfast', description:'Breakfast'}]\n\n    return (\n        <>\n        <nav>\n            <div className=\"lista\">\n                <Link to=\"/\" className=\"listado\">Home</Link>\n                <Link to=\"/aboutMe\" className=\"listado\">About Me</Link>\n                <DropdownNav/>              \n                <CartWidget/>\n            </div>\n        </nav>\n        </>\n    )\n}\n\nexport default Navbar\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/cart/CartItem.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/MealPrep/src/components/header/DropdownNav.js",["109","110","111"],"import React, { useEffect, useState } from 'react'\nimport {Dropdown} from 'react-bootstrap';\nimport { Link } from 'react-router-dom'\nimport { firestore } from '../../firebaseConfig'\n\n\n\nconst DropdownNav = () => {\n    \n    // const [categorias, setCategorias] = useState([])\n\n  